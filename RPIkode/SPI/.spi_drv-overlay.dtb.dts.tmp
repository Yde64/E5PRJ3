# 1 "/mnt/c/Users/dryos/Documents/GitHub/E5PRJ3/RPIkode/SPI/spi_drv-overlay.dts"
/dts-v1/;
/plugin/;

/ {
    compatible = "brcm,bcm2835", "brcm,bcm2708";

    fragment@0 {
        target = <&spi0>;
        __overlay__ {
            status = "okay";
            spidev@0{
                status = "disabled";
            };
        };
    };

    fragment@1 {
        target = <&spi0>;
        __overlay__ {

            #address-cells = <1>;
            #size-cells = <0>;
            spi_drv:spi_drv@0 {
                compatible = "ase, spi_drv";
                reg = <0>;
                spi-cpha;
                spi-cpol;
                spi-max-frequency = <100000>;
            };
        };
    };
    fragment@2 {

        target-path = "/";
        __overlay__ {

            plat_drv: plat_drv@0 {

                compatible = "ase, plat_drv";



                gpios = <&gpio 22 0>;

                status = "okay";
            };
        };
    };
};
